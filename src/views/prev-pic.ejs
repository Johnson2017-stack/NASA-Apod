<html>

<head>
  <link href="./public/style.css" rel="stylesheet" type="text/css" />
</head>

<body>
    <h1 id="top-title">Previous Pictures of the day</h1>
    <% nasaData.map(el => { %>
    <h2 id="title"><%= el.title %></h2>
    <h3 id="date"><%= el.date %></h3>
  
    <div id="media">
      <%if(el.media_type === "video") { %>
      <iframe width="420" height="315" src="<%=el.url%>"> </iframe>
      <% } %>
  
      <%if(el.media_type === "image") { %>
      <img src="<%=el.url%>" />
      <% } %>
    </div>
    <p><%= el.explanation %></p>
  
    <% }) %> 
  


  <!-- //navigation to home page -->
  <nav>
    <a href="/home">home</a>
  </nav>
</body>

</html>